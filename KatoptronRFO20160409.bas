IF instinctused = 0 THEN
  REM stringtolearn$ = instring$
  stringtolearn$ = machine$

  REM PRINT "got here"

  REM Now switch the references between "you" and "I" and undertake any other replacements needed.
  IF stringtolearn$ <> "" THEN
    REM the below line makes start and end matching easier:
    stringtolearn$ = " " + stringtolearn$ + " "
    changer = 1
    WHILE changer < 3030

      REM try to find the next word in the stringtolearn - if found, replace.
      foundchange = IS_IN(replarray$[changer], stringtolearn$)

      REM safety switch against case-insensitive INSTR-functionality - ignore the match if it is lowercase:
      IF foundchange <> 0 THEN
        lensecstr = LEN(replarray$[changer])
        fststr$ = MID$(stringtolearn$, foundchange, lensecstr)
        secstr$ = replarray$[changer]

        IF fststr$ <> secstr$ THEN
          foundchange = 0
        END IF
      END IF

      IF foundchange <> 0 THEN
        exchg = exchg + 1
        leftoffset = foundchange - 1
        rightoffset = LEN(stringtolearn$) - foundchange - LEN(replarray$[changer]) + 1
        leftpart$ = LEFT$(stringtolearn$, leftoffset)
        rightpart$ = RIGHT$(stringtolearn$, rightoffset)
        stringtolearn$ = leftpart$ + replarray$[changer + 1] + rightpart$
        foundchange = 0
      ELSE
        REM if the word is not found any longer, try the next key-value-pair:
        changer = changer + 2
      ENDIF
    REPEAT
    REM remove the extra white spaces for start and end matching:
    stringtolearn$ = MID$(stringtolearn$, 2, LEN(stringtolearn$) - 1)

    REM PRINT "changed stringtolearn = "; stringtolearn$

    stringtolearn$ = UPPER$(stringtolearn$)
  ENDIF

ENDIF

